{
  "name": "specialtechevents",
  "profile": "borre",
  "regions": ["eu-west-1"],
  "type": "static",
  "hooks": {
    "build": ["npm run generate"],
    "clean": "rm -fr dist"
  },
  "static": {
    "dir": "dist"
  },
  "stages": {
    "production": {
      "domain": "specialtechevents.com"
    },
    "staging": {
      "domain": "staging.specialtechevents.com"
    }
  }
}
